programa {
	funcao inicio() {
		/*crie um codigo que digite o nome da ciade conforme o número*/
		inteiro codigo 
		escreva ("informe o código da cidade")
		leia (codigo)
		escolha (codigo){
		    caso 1: 
		      escreva ("Serrita")
		      pare 
		    caso 2:
		      escreva ("Salgueiro")
		      pare
		    caso 3:
		      escreva ("Crato")
		      pare
		    caso 4:
		     escreva ("Petrolina")
		     pare
		    caso contrario:
		     escreva ("código invalido")
		     pare
		}
		
	}
}
