programa {
	funcao inicio() {
		inteiro n 
		escreva ("informe o número")
		leia (n)
		se(n == 0){
		    escreva ("\n número nulo!")
		}senao se (n% 2==0){
		    escreva ("\numero par ")
		    se (n > 20){
		        escreva ("\nnúmero maior que 20!")
		    }
		}senao se (n % 2 == 1){
		    escreva ("\nnúmeo impar")
		    se (n == 5){
		        escreva ("\nO número é 5")
		    }
		}
	}
}
